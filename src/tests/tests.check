#include "../rpr/core.h"
#include "../rpr/expr.h"
#include "../rpr/llist.h"
#include "../rpr/stack.h"
#define EPS 1e-7
#include <math.h>
#include <stdlib.h>

#test ll_list_1
struct ll_node *l = ll_node_new(1, 2.2);
uint expected_result, actual_result;
expected_result = 1;
actual_result = l->state;
ck_assert_uint_eq(expected_result, actual_result);
ck_assert(fabs(2.2 - l->datum) < EPS);
ll_destroy_list(l);

